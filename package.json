{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\" \"npm run storybook\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server",
    "server:dev:cluster": "ts-node-dev --project tsconfig.server.json src/clusters",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "storybook:build-sav": "build-storybook -o dist/storybook",
    "storybook:build": "",
    "build": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "husky",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": " npm run typeorm migration:run -- -d ./src/data-source.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.6",
    "@types/mjml": "^4.7.4",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.5.1",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "http-proxy-middleware": "^3.0.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "3.3.3",
    "storybook-builder-vite": "^0.1.23",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "stylelint-config-standard": "^36.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "ky": "^1.7.1",
    "memory-cache": "^0.2.0",
    "mjml": "^4.15.3",
    "ms": "^2.1.3",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.26.1",
    "reflect-metadata": "^0.2.2",
    "sass-embedded": "^1.77.8",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "zustand": "^4.5.5"
  }
}
